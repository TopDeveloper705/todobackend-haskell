{
    "project": "todobackend-haskell",
    "services": [
        {
            "id": "todobackends",
            "apps": [
                {
                    "id": "todobackend-scotty",
                    "domain": {
                        "type": "HTTP",
                        "uri": "todobackend-scotty.sloppy.zone"
                    },
                    "mem": 512,
                    "image": "jhedev/todobackend-haskell:scotty",
                    "instances": 1,
                    "port_mappings": [
                        {
                            "container_port": 3000
                        }
                    ],
                    "env": {
                        "URL": "http://todobackend-scotty.sloppy.zone"
                    }
                },
